        field(DTYP, "EtherIP")
}


## Belows are settign to read the analog moudle value of AB PLC.

record(ai, "$(IOC):AI_CH0")
{
        field(DESC, "Channel 0 Input value")
        field(SCAN, ".1 second")
        field(INP, "@$(PLC) Local:4:I.Ch0Data")
        field(DTYP, "EtherIP")
	
}

