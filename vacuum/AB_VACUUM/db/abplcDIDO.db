# Below are TMP remote inpus signals.

 record(bi, "$(IOC):TMP_ACCEL")
{
         field(SCAN, ".1 second")
        field(INP, "@$(PLC) TMP_ACCEL")
        field(DTYP, "EtherIP")
        field(ZNAM, "False")
        field(ONAM, "True")
 }

