##belows are string settings for XGS600.
 
record(stringin, "$(IOC):CNV2_value")
{
        field(DESC, "CNV2_PRESSURE")
        field(SCAN, ".5 second")
        field(INP, "@$(PLC) READ_CNV2_DIS")
        field(DTYP, "EtherIP")
}

